@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="reports-section">

    @*<nav class="top-nav" data-ng-controller="loginCtrl">
        <h4 class="reports-section--title"><span><i class="fa fa-list-alt"></i></span> HR Reports</h4>
        <ul class="top-nav-list">
            <li class="top-nav-item"><a href="/" class="top-nav-link"><i class="fa fa-home" aria-hidden="true"></i> Home</a></li>
            <li class="top-nav-item"><a class="top-nav-link" ng-click="logOut()"><i class="fa fa-sign-out" aria-hidden="true"></i> Sign out</a></li>
        </ul>
    </nav>*@

    <!--Navigation Bar-->
    <header class="header" ng-hide="showHideContent">
        <nav class="navbar navbar-custom" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#custom-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="index.html">
                        <p>Proximity HR</p>
                        @*<img class="navbar-brand--logo" src="../../images/Proximititranspa.png" alt="" />*@
                    </a>
                </div>
                <div class="collapse navbar-collapse" id="custom-collapse">
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="/#home"><i class="fa fa-home" aria-hidden="true"></i> Home</a></li>
                        <li class="dropdown" ng-hide="showHideAdminContent">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown"
                               role="button"
                               aria-haspopup="true"
                               aria-expanded="false"><i class="fa fa-cog" aria-hidden="true"></i>Manage<span class="caret"></span></a>
                            <ul class="dropdown-menu">
                                <li><a href="/#users"><i class="fa fa-users" aria-hidden="true"></i> Users</a></li>
                                <li><a href="/#technology"><i class="fa fa-laptop" aria-hidden="true"></i> Technology & Features</a></li>
                                <li>@Html.ActionLink("Reports", "Index", "Reports")</li>
                            </ul>
                        </li>
                        <li><a href="/#profile"><i class="fa fa-user" aria-hidden="true"></i> Profile</a></li>
                        <li><a href="/#skillSet"><i class="fa fa-code" aria-hidden="true"></i> My Skills</a></li>
                        <li><a href="" style="font-weight: bold; margin-bottom: 10px" ng-click="logOut()"><i class="fa fa-sign-out" aria-hidden="true"></i> SignOut</a></li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <div class="row">
            @*left panel*@
            <a href="#" class="js--toggle-left-panel toggle-left-panel pull-right"><i class="glyphicon glyphicon-menu-hamburger"></i></a>
            <div class="left-panel">
                <ul class="left-nav">
                    <span class="left-nav--subtitle">Menu</span>
                    <li class="left-item left-item--active"><a class="left-link" href="#intro"><i class="fa fa-bar-chart" aria-hidden="true"></i> Intro</a></li>
                    <li class="left-item"><a class="left-link" href="#average"><i class="fa fa-bar-chart" aria-hidden="true"></i> Average</a></li>
                    <li class="left-item"><a class="left-link" href="#demo"><i class="fa fa-map-marker" aria-hidden="true"></i> Demographics</a></li>
                    <li class="left-item"><a class="left-link" href="#features"><i class="fa fa-laptop" aria-hidden="true"></i> Features</a></li>
                    <li class="left-item"><a class="left-link" href="#license"><i class="glyphicon glyphicon-credit-card" aria-hidden="true"></i> Licenses</a></li>
                    <li class="left-item"><a class="left-link" href="#age"><i class="fa fa-pie-chart" aria-hidden="true"></i> Aging</a></li>
                    <li class="left-item"><a class="left-link" href="#ms"><i class="fa fa-pie-chart" aria-hidden="true"></i> Marital status</a></li> 
                </ul>
            </div>
        <div class="col-xs-12">
            @*reports*@
            <div class="container">
                
                <div class="row">
                    <div class="container">
                        <div class="col-xs-1"></div>
                        <div class="col-xs-11" id="loadHere">
                            @{ @Html.Partial("_dash"); }
                            
                            @{ @Html.Partial("_AverageReport"); }

                            @{ @Html.Partial("_DemoRep"); }

                            @{ @Html.Partial("_FeatRep"); }

                            @{ @Html.Partial("_EDRep"); }

                            @{ @Html.Partial("_AgeReport"); }

                            @{ @Html.Partial("_MSRep"); }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

